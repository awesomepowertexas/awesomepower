    "@trpc/next": "^9.20.1",
    "@trpc/react": "^9.20.1",
    "@trpc/server": "^9.20.1",
    "axios": "^0.26.1",
    "classnames": "^2.3.1",
    "decimal.js": "^10.3.1",
    "immer": "^9.0.12",
    "lodash": "^4.17.21",
    "next": "^12.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.34.16",
    "superjson": "^1.8.1",
    "use-immer": "^0.6.0",
    "zod": "^3.13.4",
    "zustand": "^3.7.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/react": "^5.12.4",
    "@cypress/webpack-dev-server": "^1.8.3",
    "@faker-js/faker": "^6.0.0",
    "@google-cloud/vision": "^2.4.2",
    "@istanbuljs/schema": "^0.1.3",
    "@svgr/webpack": "^6.2.1",
    "@tailwindcss/forms": "^0.5.0",
    "@tailwindcss/typography": "^0.5.2",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.180",
