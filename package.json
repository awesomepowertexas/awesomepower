    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "^10.4.4",
    "canvas": "^2.9.0",
    "consola": "^2.15.3",
    "csv": "^6.0.5",
    "cypress": "^9.5.2",
    "dotenv": "^16.0.0",
    "eslint": "^8.11.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
