  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit --pretty",
    "db:reset": "prisma migrate reset --force --skip-generate --skip-seed && prisma db push",
    "db:seed": "node --loader ts-node/esm scripts/seed.ts",
    "plans:update": "node --loader ts-node/esm scripts/plans/update.ts",
    "cypress:open": "cypress open",
    "cypress:open-ct": "cypress open-ct",
    "coverage:dev": "node --loader ts-node/esm scripts/coverage/instrument.ts && cd .nyc/instrumented && pnpm dev",
    "coverage:process": "node --loader ts-node/esm scripts/coverage/process.ts",
    "coverage:open": "node --loader ts-node/esm scripts/coverage/open.ts"
  },
