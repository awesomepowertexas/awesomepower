{
  "name": "awesomepower",
  "author": "Michael Hays",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=16 <17",
    "pnpm": ">=7 <8"
  },
  "volta": {
    "node": "16.15.0"
  },
  "packageManager": "pnpm@7.1.0",
  "type": "commonjs",
  "prisma": {
    "seed": "pnpm seed"
  },
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "next lint",
    "typecheck": "tsc --noEmit --pretty",
    "db:reset": "prisma migrate reset --force --skip-generate --skip-seed && prisma db push",
    "db:seed": "ts-node --files -r tsconfig-paths/register -r dotenv/config scripts/seed.ts",
    "vitest:run": "vitest run",
    "cypress:open": "cypress open",
    "cypress:open-ct": "cypress open-ct",
    "admin:updatePlans": "ts-node --files -r tsconfig-paths/register -r dotenv/config scripts/admin/updatePlans/index.ts"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-brands-svg-icons": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@fortawesome/react-fontawesome": "^0.1.18",
    "@prisma/client": "^3.14.0",
    "@trpc/client": "^9.23.2",
    "@trpc/next": "^9.23.2",
    "@trpc/react": "^9.23.2",
    "@trpc/server": "^9.23.2",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "decimal.js": "^10.3.1",
    "immer": "^9.0.14",
    "lodash": "^4.17.21",
    "next": "^12.1.6",
    "node-fetch": "^3.2.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-query": "^3.39.0",
    "superjson": "^1.9.1",
    "use-immer": "^0.7.0",
    "zod": "^3.16.0",
    "zustand": "4.0.0-rc.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/react": "^5.12.4",
    "@cypress/webpack-dev-server": "^1.8.4",
    "@faker-js/faker": "^6.3.1",
    "@google-cloud/vision": "^2.4.2",
    "@istanbuljs/schema": "^0.1.3",
    "@svgr/webpack": "^6.2.1",
    "@tailwindcss/forms": "^0.5.1",
    "@tailwindcss/typography": "^0.5.2",
    "@types/jsdom": "^16.2.14",
    "@types/lodash": "^4.14.182",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/shelljs": "^0.8.11",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.7",
    "c8": "^7.11.3",
    "canvas": "^2.9.1",
    "consola": "^2.15.3",
    "csv": "^6.0.5",
    "cypress": "^9.6.1",
    "dotenv": "^16.0.1",
    "eslint": "^8.15.0",
    "eslint-config-next": "^12.1.6",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
